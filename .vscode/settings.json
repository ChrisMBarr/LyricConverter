{
  "files.exclude": {
      "**/*.tsx": true,
      "**/*.json": false,
      "**/.gitignore": true,
      "**/.gitkeep": true,
      ".angular": true,
      ".editorconfig": true,
      ".prettierrc": true,
      "node_modules": true,
      "coverage": true
    },
    "explorer.fileNesting.patterns": {
      "tsconfig.json": "tsconfig.*.json",
      "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml"
    },
    "cSpell.words": [
      "propresenter"
    ]
}
