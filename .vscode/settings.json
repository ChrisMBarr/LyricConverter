{
  "files.exclude": {
      "**/*.tsx": true,
      "**/*.json": false,
      "**/.gitignore": true,
      "**/.gitkeep": true,
      ".angular": true,
      ".editorconfig": true,
      ".prettierrc": true,
      "coverage": true,
      "design": true,
      "dist": true,
      "node_modules": true,
    },
    "explorer.fileNesting.patterns": {
      "tsconfig.json": "tsconfig.*.json",
      "package.json": "package-lock.json"
    },
    "explorer.fileNesting.expand": false,
    "cSpell.words": [
      "CCLI",
      "chordpro",
      "openlyrics",
      "propresenter",
      "sbsong",
      "songpro",
      "songshowplus"
    ]
}
